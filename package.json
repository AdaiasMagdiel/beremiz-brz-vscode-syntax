{
  "name": "beremiz",
  "displayName": "beremiz-brz-syntax",
  "description": "Language support for Beremiz language",
  "icon": "images/icon.png",
  "version": "0.0.3",
  "engines": {
    "vscode": "^1.104.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "beremiz",
        "aliases": [
          "Beremiz",
          "beremiz"
        ],
        "extensions": [
          ".brz"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "beremiz",
        "scopeName": "source.brz",
        "path": "./syntaxes/beremiz.tmLanguage.json"
      }
    ]
  },
  "publisher": "adaias-magdiel",
  "author": {
    "name": "Ada√≠as Magdiel",
    "email": "eu@adaiasmagdiel.com",
    "url": "https://github.com/AdaiasMagdiel"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AdaiasMagdiel/beremiz-brz-vscode-syntax.git"
  }
}
